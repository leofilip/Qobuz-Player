{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "qobuz-player",
  "version": "0.1.0",
  "identifier": "com.leo.qobuz-player",
  "build": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "url": "https://play.qobuz.com/", 
        "title": "Qobuz Desktop Player",
        "width": 1200,
        "height": 700,
        "minWidth": 1200,
        "minHeight": 700,
        "resizable": true,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
